@@ -0,0 +1,646 @@
+import fontkit from "@pdf-lib/fontkit";
+import { PDFDocument, rgb, type PDFFont, type PDFPage } from "pdf-lib";
+import type { DocumentSectionItem, PlatformDocument } from "../documents-store";
+import type { PlatformRequest } from "../requests-store";
+import { loadNordicFontBytes } from "./font-loader";
+
+interface RenderLineItem {
+  moment: string;
+  ref: string;
+  note: string;
+  amount?: number;
+}
+
+interface RenderSection {
+  title: string;
+  rows: Array<{ label: string; value: string }>;
+  bullets: string[];
+}
+
+export interface DocumentRenderModel {
+  localeSample: string;
+  docTypeLabel: string;
+  generatedAt: string;
+  projectRows: Array<{ label: string; value: string }>;
+  summaryRows: Array<{ label: string; value: string }>;
+  sections: RenderSection[];
+  lineItems: RenderLineItem[];
+}
+
+const A4_WIDTH = 595;
+const A4_HEIGHT = 842;
+const MARGIN = 30;
+const CONTENT_WIDTH = A4_WIDTH - MARGIN * 2;
+const FOOTER_HEIGHT = 24;
+const HEADER_HEIGHT = 62;
+
+const C = {
+  text: rgb(0.13, 0.13, 0.13),
+  muted: rgb(0.45, 0.45, 0.45),
+  line: rgb(0.86, 0.86, 0.86),
+  panel: rgb(0.97, 0.97, 0.97),
+  panelBorder: rgb(0.82, 0.82, 0.82),
+  brand: rgb(0.26, 0.23, 0.2),
+};
+
+function typeLabel(type: PlatformDocument["type"]): string {
+  if (type === "quote") return "OFFERT";
+  if (type === "contract") return "AVTAL";
+  return "ÄTA";
+}
+
+function folderSafeProjectName(input: string): string {
+  return input
+    .normalize("NFKD")
+    .replaceAll(/[\u0300-\u036f]/g, "")
+    .replaceAll(/[^a-zA-Z0-9]+/g, "_")
+    .replaceAll(/^_+|_+$/g, "")
+    .slice(0, 50);
+}
+
+export function buildDocumentPdfFilename(document: PlatformDocument, request: PlatformRequest | null): string {
+  const prefix = document.type === "quote" ? "Offert" : document.type === "contract" ? "Avtal" : "ATA";
+  const date = new Date(document.updatedAt || document.createdAt)
+    .toISOString()
+    .slice(0, 10);
+  const projectName = folderSafeProjectName(request?.title || document.title || document.requestId) || "Projekt";
+  return `${prefix}_${projectName}_${date}_v${document.version}.pdf`;
+}
+
+function formatDateSv(input: string | undefined): string {
+  if (!input) return "-";
+  const parsed = new Date(input);
+  if (Number.isNaN(parsed.getTime())) return input;
+  return parsed.toLocaleDateString("sv-SE");
+}
+
+function parseNumberLike(value: string): number | null {
+  const cleaned = value.replaceAll(/[\s'´]/g, "").replaceAll(",", ".");
+  if (!/^\d+(\.\d+)?$/.test(cleaned)) return null;
+  const parsed = Number(cleaned);
+  return Number.isFinite(parsed) ? parsed : null;
+}
+
+function formatSek(amount: number): string {
+  return `${new Intl.NumberFormat("sv-SE", {
+    maximumFractionDigits: 0,
+    minimumFractionDigits: 0,
+  }).format(amount)} kr`;
+}
+
+function normalizeCurrencyRange(input: string): string {
+  const tokens = input.match(/[\d][\d\s'´,.]*/g) ?? [];
+  const parsed = tokens
+    .map((token) => parseNumberLike(token))
+    .filter((n): n is number => n !== null);
+  if (parsed.length === 0) return input;
+  if (parsed.length === 1) return formatSek(parsed[0]);
+  return `${formatSek(parsed[0]).replace(" kr", "")}–${formatSek(parsed[1])}`;
+}
+
+function toRowValue(value: string | number | boolean | undefined): string {
+  if (typeof value === "boolean") return value ? "Ja" : "Nej";
+  if (typeof value === "number") return `${value}`;
+  if (typeof value !== "string") return "-";
+  const trimmed = value.trim();
+  if (!trimmed) return "-";
+  return trimmed;
+}
+
+function sectionOrderScore(title: string): number {
+  const t = title.toLowerCase();
+  if (t.includes("projektöversikt")) return 10;
+  if (t.includes("omfattning")) return 20;
+  if (t.includes("tidplan")) return 30;
+  if (t.includes("pris") || t.includes("betalning")) return 40;
+  if (t.includes("förutsätt") || t.includes("reservation")) return 50;
+  if (t.includes("garanti") || t.includes("försäkring")) return 60;
+  if (t.includes("bilag")) return 70;
+  return 100;
+}
+
+function itemToLine(item: DocumentSectionItem): RenderLineItem {
+  return {
+    moment: item.label || "Moment",
+    ref: item.description || "-",
+    note: item.value || "-",
+    amount: typeof item.total === "number" ? item.total : typeof item.unitPrice === "number" ? item.unitPrice : undefined,
+  };
+}
+
+export function buildDocumentRenderModel(input: {
+  document: PlatformDocument;
+  request: PlatformRequest | null;
+  generatedAtIso?: string;
+}): DocumentRenderModel {
+  const { document, request } = input;
+  const generatedAt = input.generatedAtIso ?? document.updatedAt ?? document.createdAt;
+
+  const projectRows: Array<{ label: string; value: string }> = [
+    { label: "Projekt", value: request?.title || document.title },
+    { label: "Plats", value: request?.location || "-" },
+    { label: "Request-ID", value: document.requestId },
+    { label: "Dokument-ID", value: document.id },
+    { label: "Dokumenttyp", value: typeLabel(document.type) },
+    { label: "Version", value: `${document.version}` },
+    { label: "Skapad", value: formatDateSv(document.createdAt) },
+    { label: "Uppdaterad", value: formatDateSv(document.updatedAt) },
+  ];
+
+  const totalFromItems = document.sections
+    .flatMap((section) => section.items ?? [])
+    .reduce((sum, item) => sum + (typeof item.total === "number" ? item.total : 0), 0);
+
+  const summaryRows: Array<{ label: string; value: string }> = [
+    { label: "Budget", value: normalizeCurrencyRange(request?.budgetRange ?? "-") },
+    { label: "Önskad start", value: toRowValue(request?.desiredStart) },
+    {
+      label: "Summerat belopp",
+      value: totalFromItems > 0 ? formatSek(totalFromItems) : "Specificeras i handling",
+    },
+    { label: "Betalning", value: "Enligt avtalad betalningsplan" },
+  ];
+
+  const sections: RenderSection[] = document.sections
+    .filter((section) => section.enabled)
+    .map((section) => ({
+      title: section.title,
+      rows: section.fields.map((field) => ({
+        label: field.label,
+        value: toRowValue(field.value),
+      })),
+      bullets: (section.items ?? []).map((item) => {
+        const parts = [
+          item.description,
+          typeof item.quantity === "number" ? `Antal ${item.quantity}` : undefined,
+          typeof item.unitPrice === "number" ? `Á-pris ${formatSek(item.unitPrice)}` : undefined,
+          typeof item.total === "number" ? `Belopp ${formatSek(item.total)}` : undefined,
+          item.value,
+        ].filter((p): p is string => Boolean(p));
+        return parts.length > 0 ? `${item.label}: ${parts.join(" · ")}` : item.label;
+      }),
+    }))
+    .sort((a, b) => sectionOrderScore(a.title) - sectionOrderScore(b.title));
+
+  const lineItems = document.sections
+    .filter((section) => section.enabled)
+    .flatMap((section) => section.items ?? [])
+    .map(itemToLine);
+
+  return {
+    localeSample: "ÅÄÖ åäö",
+    docTypeLabel: typeLabel(document.type),
+    generatedAt,
+    projectRows,
+    summaryRows,
+    sections,
+    lineItems,
+  };
+}
+
+function wrapText(text: string, font: PDFFont, size: number, maxWidth: number): string[] {
+  const normalized = text.replaceAll("\n", " ").replaceAll(/\s+/g, " ").trim();
+  if (!normalized) return [""];
+
+  const words = normalized.split(" ");
+  const lines: string[] = [];
+  let current = "";
+
+  words.forEach((word) => {
+    const candidate = current ? `${current} ${word}` : word;
+    if (font.widthOfTextAtSize(candidate, size) <= maxWidth) {
+      current = candidate;
+      return;
+    }
+
+    if (current) {
+      lines.push(current);
+      current = word;
+      return;
+    }
+
+    let chunk = "";
+    for (const char of word) {
+      const next = `${chunk}${char}`;
+      if (font.widthOfTextAtSize(next, size) > maxWidth && chunk) {
+        lines.push(chunk);
+        chunk = char;
+      } else {
+        chunk = next;
+      }
+    }
+    current = chunk;
+  });
+
+  if (current) lines.push(current);
+  return lines;
+}
+
+export async function renderDocumentToPdfBytes(input: {
+  document: PlatformDocument;
+  request: PlatformRequest | null;
+  generatedAtIso?: string;
+  fontBytes?: Uint8Array;
+}): Promise<Uint8Array> {
+  const model = buildDocumentRenderModel(input);
+  const pdfDoc = await PDFDocument.create();
+  pdfDoc.registerFontkit(fontkit);
+
+  const fontBytes = input.fontBytes ?? (await loadNordicFontBytes());
+  const font = await pdfDoc.embedFont(fontBytes, { subset: true });
+
+  pdfDoc.setTitle(`${model.docTypeLabel} ${input.document.title}`);
+  pdfDoc.setCreator("Byggplattformen");
+  pdfDoc.setProducer("Byggplattformen PDF Renderer");
+  pdfDoc.setKeywords(["embedded-font:noto-sans-regular", model.localeSample]);
+
+  const pages: PDFPage[] = [];
+  const addPage = (): { page: PDFPage; topY: number } => {
+    const page = pdfDoc.addPage([A4_WIDTH, A4_HEIGHT]);
+    pages.push(page);
+    return { page, topY: A4_HEIGHT - MARGIN - HEADER_HEIGHT };
+  };
+
+  const firstPage = addPage();
+  let page = firstPage.page;
+  let y = firstPage.topY;
+
+  const bodySize = 10.5;
+  const h1 = 20;
+  const h2 = 12.5;
+  const metaSize = 8.5;
+  const rowGap = 4;
+  const lineHeight = 13;
+
+  const drawHeader = (target: PDFPage, firstPage: boolean) => {
+    target.drawLine({
+      start: { x: MARGIN, y: A4_HEIGHT - MARGIN - HEADER_HEIGHT + 8 },
+      end: { x: A4_WIDTH - MARGIN, y: A4_HEIGHT - MARGIN - HEADER_HEIGHT + 8 },
+      thickness: 0.7,
+      color: C.line,
+    });
+
+    target.drawText("Byggplattformen", {
+      x: MARGIN,
+      y: A4_HEIGHT - MARGIN - 18,
+      size: firstPage ? h1 : 13,
+      font,
+      color: C.brand,
+    });
+
+    target.drawText("Projektdokument", {
+      x: MARGIN,
+      y: A4_HEIGHT - MARGIN - 33,
+      size: 9,
+      font,
+      color: C.muted,
+    });
+
+    const boxWidth = 175;
+    const boxHeight = firstPage ? 44 : 34;
+    const boxX = A4_WIDTH - MARGIN - boxWidth;
+    const boxY = A4_HEIGHT - MARGIN - boxHeight;
+
+    target.drawRectangle({
+      x: boxX,
+      y: boxY,
+      width: boxWidth,
+      height: boxHeight,
+      borderColor: C.panelBorder,
+      borderWidth: 1,
+      color: C.panel,
+    });
+
+    target.drawText(model.docTypeLabel, {
+      x: boxX + 10,
+      y: boxY + boxHeight - 15,
+      size: 11,
+      font,
+      color: C.text,
+    });
+
+    target.drawText(`Datum ${formatDateSv(model.generatedAt)}`, {
+      x: boxX + 10,
+      y: boxY + 9,
+      size: 8.5,
+      font,
+      color: C.muted,
+    });
+  };
+
+  const createNewPage = () => {
+    const created = addPage();
+    page = created.page;
+    y = created.topY;
+    drawHeader(page, false);
+  };
+
+  const ensureSpace = (height: number) => {
+    if (y - height < MARGIN + FOOTER_HEIGHT) {
+      createNewPage();
+    }
+  };
+
+  const drawWrappedText = (text: string, x: number, width: number, size: number, color = C.text): number => {
+    const lines = wrapText(text, font, size, width);
+    lines.forEach((line) => {
+      page.drawText(line, {
+        x,
+        y,
+        size,
+        font,
+        color,
+      });
+      y -= lineHeight;
+    });
+    return lines.length;
+  };
+
+  const drawSectionHeading = (title: string) => {
+    ensureSpace(lineHeight * 2);
+    page.drawLine({
+      start: { x: MARGIN, y: y + 4 },
+      end: { x: A4_WIDTH - MARGIN, y: y + 4 },
+      thickness: 0.7,
+      color: C.line,
+    });
+    y -= 10;
+    page.drawText(title, {
+      x: MARGIN,
+      y,
+      size: h2,
+      font,
+      color: C.text,
+    });
+    y -= lineHeight;
+  };
+
+  const drawProjectBlock = () => {
+    const blockTop = y;
+    const blockWidth = CONTENT_WIDTH;
+    const rowHeight = 14;
+    const rows = model.projectRows;
+    const cols = 2;
+    const rowsPerCol = Math.ceil(rows.length / cols);
+    const blockHeight = rowsPerCol * rowHeight + 16;
+
+    ensureSpace(blockHeight + 8);
+
+    page.drawRectangle({
+      x: MARGIN,
+      y: y - blockHeight + 8,
+      width: blockWidth,
+      height: blockHeight,
+      borderWidth: 1,
+      borderColor: C.panelBorder,
+      color: C.panel,
+    });
+
+    const colWidth = blockWidth / 2 - 12;
+    for (let col = 0; col < cols; col++) {
+      const start = col * rowsPerCol;
+      const part = rows.slice(start, start + rowsPerCol);
+      let rowY = blockTop - 6;
+      const colX = MARGIN + 10 + col * (colWidth + 12);
+      part.forEach((row) => {
+        page.drawText(`${row.label}:`, {
+          x: colX,
+          y: rowY,
+          size: metaSize,
+          font,
+          color: C.muted,
+        });
+        const valueLines = wrapText(row.value, font, bodySize, colWidth - 54);
+        page.drawText(valueLines[0] ?? "-", {
+          x: colX + 50,
+          y: rowY,
+          size: bodySize,
+          font,
+          color: C.text,
+        });
+        rowY -= rowHeight;
+      });
+    }
+
+    y -= blockHeight + 10;
+  };
+
+  const drawSummaryPanel = () => {
+    const panelWidth = 220;
+    const panelHeight = model.summaryRows.length * 16 + 16;
+    ensureSpace(panelHeight + 8);
+
+    const x = A4_WIDTH - MARGIN - panelWidth;
+    const y0 = y - panelHeight + 8;
+
+    page.drawRectangle({
+      x,
+      y: y0,
+      width: panelWidth,
+      height: panelHeight,
+      borderWidth: 1,
+      borderColor: C.panelBorder,
+      color: C.panel,
+    });
+
+    let rowY = y - 6;
+    model.summaryRows.forEach((row) => {
+      page.drawText(row.label, {
+        x: x + 10,
+        y: rowY,
+        size: metaSize,
+        font,
+        color: C.muted,
+      });
+      const value = row.value;
+      const textWidth = font.widthOfTextAtSize(value, bodySize);
+      page.drawText(value, {
+        x: x + panelWidth - 10 - textWidth,
+        y: rowY,
+        size: bodySize,
+        font,
+        color: C.text,
+      });
+      rowY -= 16;
+    });
+
+    y -= panelHeight + 10;
+  };
+
+  const drawLineItemsTable = () => {
+    if (model.lineItems.length === 0) return;
+
+    drawSectionHeading("Omfattning och ingående moment");
+
+    const tableX = MARGIN;
+    const widths = [210, 110, 155, 60];
+    const headerHeight = 18;
+
+    const drawTableHeader = () => {
+      ensureSpace(headerHeight + 6);
+      page.drawRectangle({
+        x: tableX,
+        y: y - headerHeight + 4,
+        width: widths.reduce((a, b) => a + b, 0),
+        height: headerHeight,
+        color: C.panel,
+        borderColor: C.panelBorder,
+        borderWidth: 1,
+      });
+
+      const labels = ["Moment/Åtgärd", "Källa/Ref", "Notering/Tagg", "Belopp"];
+      let x = tableX + 6;
+      labels.forEach((label, idx) => {
+        page.drawText(label, {
+          x,
+          y: y - 10,
+          size: metaSize,
+          font,
+          color: C.muted,
+        });
+        x += widths[idx];
+      });
+      y -= headerHeight;
+    };
+
+    drawTableHeader();
+
+    model.lineItems.forEach((item, rowIndex) => {
+      const colText = [item.moment, item.ref, item.note, item.amount ? formatSek(item.amount) : "-"];
+      const colLines = [
+        wrapText(colText[0], font, bodySize, widths[0] - 8),
+        wrapText(colText[1], font, bodySize, widths[1] - 8),
+        wrapText(colText[2], font, bodySize, widths[2] - 8),
+        wrapText(colText[3], font, bodySize, widths[3] - 8),
+      ];
+
+      const rowHeight = Math.max(...colLines.map((lines) => lines.length)) * lineHeight + 4;
+      if (y - rowHeight < MARGIN + FOOTER_HEIGHT) {
+        createNewPage();
+        drawSectionHeading("Omfattning och ingående moment (forts.)");
+        drawTableHeader();
+      }
+
+      const rowYTop = y;
+      page.drawRectangle({
+        x: tableX,
+        y: y - rowHeight,
+        width: widths.reduce((a, b) => a + b, 0),
+        height: rowHeight,
+        color: rowIndex % 2 === 0 ? rgb(1, 1, 1) : rgb(0.985, 0.985, 0.985),
+        borderColor: C.line,
+        borderWidth: 0.5,
+      });
+
+      let cx = tableX + 4;
+      colLines.forEach((lines, colIndex) => {
+        lines.forEach((line, lineIndex) => {
+          const yLine = rowYTop - 12 - lineIndex * lineHeight;
+          const isAmount = colIndex === 3;
+          if (isAmount) {
+            const tw = font.widthOfTextAtSize(line, bodySize);
+            page.drawText(line, {
+              x: cx + widths[colIndex] - 8 - tw,
+              y: yLine,
+              size: bodySize,
+              font,
+              color: C.text,
+            });
+          } else {
+            page.drawText(line, {
+              x: cx,
+              y: yLine,
+              size: bodySize,
+              font,
+              color: C.text,
+            });
+          }
+        });
+        cx += widths[colIndex];
+      });
+
+      y -= rowHeight;
+    });
+
+    y -= 8;
+  };
+
+  const drawSections = () => {
+    model.sections.forEach((section) => {
+      if (section.title.toLowerCase().includes("omfattning") && model.lineItems.length > 0) {
+        return;
+      }
+
+      drawSectionHeading(section.title);
+
+      section.rows.forEach((row) => {
+        ensureSpace(lineHeight * 2);
+        page.drawText(`${row.label}:`, {
+          x: MARGIN,
+          y,
+          size: metaSize,
+          font,
+          color: C.muted,
+        });
+        const labelWidth = 120;
+        drawWrappedText(row.value, MARGIN + labelWidth, CONTENT_WIDTH - labelWidth, bodySize);
+        y -= rowGap;
+      });
+
+      section.bullets.forEach((bullet) => {
+        ensureSpace(lineHeight * 2);
+        page.drawText("•", {
+          x: MARGIN,
+          y,
+          size: bodySize,
+          font,
+          color: C.text,
+        });
+        drawWrappedText(bullet, MARGIN + 12, CONTENT_WIDTH - 12, bodySize);
+        y -= rowGap;
+      });
+
+      y -= 4;
+    });
+  };
+
+  drawHeader(page, true);
+  drawProjectBlock();
+  drawSummaryPanel();
+  drawLineItemsTable();
+  drawSections();
+
+  const totalPages = pages.length;
+  pages.forEach((p, idx) => {
+    const pageIndex = idx + 1;
+    const footerY = MARGIN - 6;
+
+    p.drawLine({
+      start: { x: MARGIN, y: footerY + 12 },
+      end: { x: A4_WIDTH - MARGIN, y: footerY + 12 },
+      thickness: 0.6,
+      color: C.line,
+    });
+
+    const left = `Dokument-ID ${input.document.id} · Projekt-ID ${input.document.requestId}`;
+    p.drawText(left, {
+      x: MARGIN,
+      y: footerY,
+      size: metaSize,
+      font,
+      color: C.muted,
+    });
+
+    const right = `Sida ${pageIndex}/${totalPages} · Genererad ${formatDateSv(model.generatedAt)}`;
+    const tw = font.widthOfTextAtSize(right, metaSize);
+    p.drawText(right, {
+      x: A4_WIDTH - MARGIN - tw,
+      y: footerY,
+      size: metaSize,
+      font,
+      color: C.muted,
+    });
+  });
+
+  return pdfDoc.save({ useObjectStreams: false });
+}
diff --git a/app/lib/project-files/content.ts b/app/lib/project-files/content.ts
new file mode 100644
index 0000000..4ead6ab
--- /dev/null
+++ b/app/lib/project-files/content.ts
@@ -0,0 +1,189 @@
+import type { ProjectFileContentRef } from "./types";
+
+const IDB_DB_NAME = "byggplattformen-project-files-content";
+const IDB_STORE_NAME = "content";
+const LOCAL_FALLBACK_KEY = "byggplattformen-project-files-content-local";
+
+/**
+ * Notes:
+ * localStorage har liten kvot och base64 växer filstorleken.
+ * Därför används IndexedDB först och localStorage endast som fallback
+ * med hård gräns för att undvika att hela appens lagring blockeras.
+ */
+export const LOCAL_STORAGE_CONTENT_MAX_BYTES = 2_500_000;
+
+interface LocalContentEntry {
+  contentId: string;
+  mimeType: string;
+  size: number;
+  base64: string;
+}
+
+function nextId(prefix: string): string {
+  return `${prefix}-${Date.now()}-${Math.random().toString(36).slice(2, 8)}`;
+}
+
+function bytesToBase64(bytes: Uint8Array): string {
+  let binary = "";
+  const chunk = 0x8000;
+  for (let i = 0; i < bytes.length; i += chunk) {
+    const part = bytes.subarray(i, i + chunk);
+    binary += String.fromCharCode(...part);
+  }
+  return btoa(binary);
+}
+
+function base64ToBytes(base64: string): Uint8Array {
+  const binary = atob(base64);
+  const bytes = new Uint8Array(binary.length);
+  for (let i = 0; i < binary.length; i++) {
+    bytes[i] = binary.charCodeAt(i);
+  }
+  return bytes;
+}
+
+function readLocalFallbackMap(): Record<string, LocalContentEntry> {
+  if (typeof window === "undefined") return {};
+  const raw = localStorage.getItem(LOCAL_FALLBACK_KEY);
+  if (!raw) return {};
+  try {
+    const parsed = JSON.parse(raw) as Record<string, LocalContentEntry>;
+    return parsed && typeof parsed === "object" ? parsed : {};
+  } catch {
+    return {};
+  }
+}
+
+function writeLocalFallbackMap(map: Record<string, LocalContentEntry>) {
+  if (typeof window === "undefined") return;
+  localStorage.setItem(LOCAL_FALLBACK_KEY, JSON.stringify(map));
+}
+
+function openContentDb(): Promise<IDBDatabase> {
+  return new Promise((resolve, reject) => {
+    if (typeof indexedDB === "undefined") {
+      reject(new Error("IndexedDB är inte tillgängligt."));
+      return;
+    }
+
+    const req = indexedDB.open(IDB_DB_NAME, 1);
+    req.onupgradeneeded = () => {
+      const db = req.result;
+      if (!db.objectStoreNames.contains(IDB_STORE_NAME)) {
+        db.createObjectStore(IDB_STORE_NAME);
+      }
+    };
+    req.onsuccess = () => resolve(req.result);
+    req.onerror = () => reject(req.error ?? new Error("Kunde inte öppna IndexedDB."));
+  });
+}
+
+async function putInIndexedDb(contentId: string, bytes: Uint8Array): Promise<void> {
+  const db = await openContentDb();
+  await new Promise<void>((resolve, reject) => {
+    const tx = db.transaction(IDB_STORE_NAME, "readwrite");
+    const store = tx.objectStore(IDB_STORE_NAME);
+    const req = store.put(bytes.buffer, contentId);
+    req.onsuccess = () => resolve();
+    req.onerror = () => reject(req.error ?? new Error("Kunde inte skriva filinnehåll till IndexedDB."));
+  });
+  db.close();
+}
+
+async function readFromIndexedDb(contentId: string): Promise<Uint8Array | null> {
+  const db = await openContentDb();
+  try {
+    const value = await new Promise<ArrayBuffer | null>((resolve, reject) => {
+      const tx = db.transaction(IDB_STORE_NAME, "readonly");
+      const store = tx.objectStore(IDB_STORE_NAME);
+      const req = store.get(contentId);
+      req.onsuccess = () => resolve((req.result as ArrayBuffer | undefined) ?? null);
+      req.onerror = () => reject(req.error ?? new Error("Kunde inte läsa filinnehåll från IndexedDB."));
+    });
+    return value ? new Uint8Array(value) : null;
+  } finally {
+    db.close();
+  }
+}
+
+async function deleteFromIndexedDb(contentId: string): Promise<void> {
+  const db = await openContentDb();
+  await new Promise<void>((resolve, reject) => {
+    const tx = db.transaction(IDB_STORE_NAME, "readwrite");
+    const store = tx.objectStore(IDB_STORE_NAME);
+    const req = store.delete(contentId);
+    req.onsuccess = () => resolve();
+    req.onerror = () => reject(req.error ?? new Error("Kunde inte radera filinnehåll från IndexedDB."));
+  });
+  db.close();
+}
+
+export async function storeContent(
+  bytes: Uint8Array,
+  mimeType: string
+): Promise<ProjectFileContentRef> {
+  const contentId = nextId("file-content");
+
+  try {
+    await putInIndexedDb(contentId, bytes);
+    return {
+      storage: "idb",
+      contentId,
+      mimeType,
+      size: bytes.byteLength,
+    };
+  } catch {
+    if (bytes.byteLength > LOCAL_STORAGE_CONTENT_MAX_BYTES) {
+      throw new Error(
+        `Filen är för stor för lokal fallback-lagring (${bytes.byteLength} bytes).`
+      );
+    }
+
+    const map = readLocalFallbackMap();
+    map[contentId] = {
+      contentId,
+      mimeType,
+      size: bytes.byteLength,
+      base64: bytesToBase64(bytes),
+    };
+    writeLocalFallbackMap(map);
+
+    return {
+      storage: "localStorage",
+      contentId,
+      mimeType,
+      size: bytes.byteLength,
+    };
+  }
+}
+
+export async function readContent(ref: ProjectFileContentRef): Promise<Uint8Array | null> {
+  if (ref.storage === "idb") {
+    try {
+      return await readFromIndexedDb(ref.contentId);
+    } catch {
+      return null;
+    }
+  }
+
+  const map = readLocalFallbackMap();
+  const entry = map[ref.contentId];
+  if (!entry?.base64) return null;
+  return base64ToBytes(entry.base64);
+}
+
+export async function deleteContent(ref: ProjectFileContentRef): Promise<void> {
+  if (ref.storage === "idb") {
+    try {
+      await deleteFromIndexedDb(ref.contentId);
+    } catch {
+      return;
+    }
+    return;
+  }
+
+  const map = readLocalFallbackMap();
+  if (!map[ref.contentId]) return;
+  delete map[ref.contentId];
+  writeLocalFallbackMap(map);
+}
